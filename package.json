{
    "name": "clawlogic",
    "version": "0.0.1",
    "private": true,
    "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "dev:sdk": "pnpm --filter @clawlogic/sdk dev",
    "agent:alpha": "pnpm --filter @clawlogic/agent agent:alpha",
    "agent:beta": "pnpm --filter @clawlogic/agent agent:beta",
    "agent:human": "pnpm --filter @clawlogic/agent agent:human",
    "sdk:sync-web-dep": "node scripts/sync-sdk-version.mjs",
    "sdk:check-web-dep": "node scripts/sync-sdk-version.mjs --check",
    "skill:sync-web-doc": "node scripts/sync-skill-md.mjs",
    "skill:check-web-doc": "node scripts/sync-skill-md.mjs --check"
  },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.4.0"
    }
}
